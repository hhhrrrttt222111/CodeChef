//This is the solution to REMOVE BAD ELEMENTS problem on CODECHEF
#include <bits/stdc++.h>
using namespace std;
int mostFrequent(int arr[], int n)
{
    sort(arr, arr + n);
 
    // Find the max frequency using linear traversal
    int max_count = 1, res = arr[0], curr_count = 1;
    for (int i = 1; i < n; i++) {
        if (arr[i] == arr[i - 1])
            curr_count++;
        else
            curr_count = 1;
       
        if (curr_count > max_count) {
            max_count = curr_count;
            res = arr[i - 1];
        }
    }
 
    return res;
}

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n, b=0;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int k = sizeof(a)/sizeof(a[0]);
	    int x = mostFrequent(a, n);
	    for(int i=0;i<n;i++){
	        if(a[i]!=x){
                b=b+1;
	        }
	    }
	    cout<<b<<endl;
	}
	return 0;
}
